{
  "name": "flight-club",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisreddington/flight-club.git"
  },
  "author": "Chris Reddington",
  "homepage": "https://github.com/chrisreddington/flight-club",
  "bugs": {
    "url": "https://github.com/chrisreddington/flight-club/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "debt:check": "npm run debt:unused && npm run debt:exports && npm run debt:deps && npm run debt:circular",
    "debt:unused": "knip --exclude duplicates",
    "debt:exports": "ts-prune --project tsconfig.json",
    "debt:deps": "depcheck --ignores='@types/*,eslint-*,@eslint/*,vitest,@vitest/*'",
    "debt:circular": "madge --circular --extensions ts,tsx src/"
  },
  "dependencies": {
    "@github/copilot-sdk": "^0.1.23",
    "@monaco-editor/react": "^4.7.0",
    "@primer/css": "^22.1.0",
    "@primer/octicons-react": "^19.21.2",
    "@primer/react": "^38.11.0",
    "next": "16.1.6",
    "octokit": "^5.0.5",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.17",
    "depcheck": "^1.4.7",
    "eslint": "^10",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "knip": "^5.83.1",
    "madge": "^8.0.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "description": "AI-powered developer growth companion - personalized coding challenges and tips based on GitHub activity"
}
